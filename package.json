    "axios": "^1.1.2",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^1.2.1",
    "concurrently": "^7.4.0",
    "isbot": "^3.6.1",
    "playwright": "^1.27.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.1.8",
    "tw-elements": "^1.0.0-alpha12"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
